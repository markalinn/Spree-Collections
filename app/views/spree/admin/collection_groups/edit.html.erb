<%= form_for(:collection_group, :url => admin_collection_group_url, :html => { :method => :put}) do |f| %>
  <table class="basic-table">
    <tr>
      <td><%= t("name") %>:</td>
    </tr>
    <%= render :partial => "form", :locals => { :f => f } %>
  </table>
  <p class="form-buttons">
    <%= button t("update") %>
    <%= t('or') %> <%= link_to t("cancel"), admin_collection_group_url, :id => "cancel_link" %>
  </p>
<% end %>
